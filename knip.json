{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["app/**/*.{ts,tsx}", "modules/**/*.{ts,tsx}"],
  "project": ["app/**/*.{ts,tsx}", "modules/**/*.{ts,tsx}"],
  "ignore": ["**/*.d.ts"],
  "ignoreDependencies": ["tailwindcss", "web-push", "workbox-window"],
  "next": {
    "entry": [
      "next.config.{js,ts,mjs}",
      "app/**/page.tsx",
      "app/**/layout.tsx",
      "app/**/loading.tsx",
      "app/**/error.tsx",
      "app/**/not-found.tsx",
      "middleware.ts"
    ]
  }
}
