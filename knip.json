{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/app/**/*.{ts,tsx}",
    "src/app/**/page.tsx",
    "src/app/**/layout.tsx"
  ],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": ["**/*.d.ts"],
  "ignoreDependencies": ["tailwindcss", "postcss-load-config"],
  "next": {
    "entry": [
      "next.config.{js,ts,mjs}",
      "src/app/**/page.tsx",
      "src/app/**/layout.tsx",
      "src/app/**/loading.tsx",
      "src/app/**/error.tsx",
      "src/app/**/not-found.tsx",
      "src/middleware.ts"
    ]
  }
}
